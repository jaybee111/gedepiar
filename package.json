{
    "name": "gedepiar",
    "version": "0.1.14",
    "description": "A dependency-free JavaScript-Tool for managing your cookies of your website/webapp. Based on GDPR-Guidelines.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "serve": "webpack-dev-server --config webpack.dev.js --mode development",
        "build": "webpack --config webpack.prod.js --mode production",
        "preview": "npm run build && http-server dist",
        "version-patch": "npm version patch --force",
        "version-minor": "cross-env npm version minor && git add . && git commit -am 'v$npm_package_version'",
        "version-major": "cross-env npm version major && git add . && git commit -am 'v$npm_package_version'",
        "publish-release": "cross-env git tag v$npm_package_version && git push origin v$npm_package_version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jaybee111/gedepiar.git"
    },
    "author": "Jonas BÃ¶ttcher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jaybee111/gedepiar/issues"
    },
    "homepage": "https://github.com/jaybee111/gedepiar#readme",
    "devDependencies": {
        "cross-env": "^7.0.2",
        "@babel/core": "^7.7.2",
        "@babel/preset-env": "^7.7.1",
        "@fullhuman/postcss-purgecss": "^1.3.0",
        "ajv": "^6.10.2",
        "autoprefixer": "^9.7.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "cssnano": "^4.1.10",
        "cypress": "^3.8.3",
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.18.2",
        "favicons-webpack-plugin": "^1.0.2",
        "file-loader": "^4.3.0",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^8.0.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.0.0",
        "url-loader": "^2.3.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "es6": true,
            "browser": true
        },
        "extends": [
            "airbnb-base"
        ],
        "parser": "babel-eslint",
        "parserOptions": {
            "sourceType": "module"
        },
        "rules": {
            "linebreak-style": 0
        }
    },
    "browserslist": [
        "defaults"
    ]
}
